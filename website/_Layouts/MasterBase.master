<%@ Master Language="C#" AutoEventWireup="true" CodeFile="MasterBase.master.cs" Inherits="_Layouts_MasterBase" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Advanced Purchasing Quote Management</title>
    <style type="text/css">
        .Overlay
        {
            position: fixed;
            top: 0px;
            bottom: 0px;
            left: 0px;
            right: 0px;
            overflow: hidden;
            padding: 0;
            margin: 0;
            background-color: #000;
            filter: alpha(opacity=50);
            opacity: .5;
            z-index: 1000;
        }
        .PopUpPanel
        {
            position: absolute;
            background-color: #B9B9B9;
            left: 38%;
            top: 50%;
            height: 330px;
            width: 1030px;
            margin: -15% 0 0 -25%;
            z-index: 2001;
            padding: 10px;
            box-shadow: 0px 2px 25px 5px #000000;
            border-radius: 10px;
            -moz-border-radiux: 5px;
            border: 1px solid #CCCCCC;
        }
        .OverlayCalendar
        {
            position: fixed;
            top: 0px;
            bottom: 0px;
            left: 0px;
            right: 0px;
            overflow: hidden;
            padding: 0;
            margin: 0;
            background-color: #000;
            filter: alpha(opacity=50);
            opacity: .5;
            z-index: 2005;
        }
        .PopUpCalendar
        {
            position: absolute;
            background-color: #B9B9B9;
            left: 38%;
            top: 50%;
            height: 200px;
            width: 260px;
            margin: -15% 0 0 -25%;
            z-index: 2006;
            padding: 10px;
            box-shadow: 0px 2px 25px 5px #000000;
            border-radius: 10px;
            -moz-border-radiux: 5px;
            border: 1px solid #CCCCCC;
        }
        .mainSection
        {
            height: 340px;
            overflow-y: auto;
            align: center;
        }
        .Content
        {
            position: relative;
        }
        .popupContainer
        {
            font-family: initial;
            font-size: initial;
        }
        .no-close .ui-dialog-titlebar-close
        {
            display: none;
        }
        ::-ms-clear
        {
            display: none;
        }
    </style>
    <style type="text/css" title="currentStyle">
        @import '<%= this.ResolveUrl("~/Scripts/jQuery/DataTable/media/css/jquery.dataTables_themeroller.css") %>';
        @import '<%= this.ResolveUrl("~/Tab.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/jQuery/Dialog/css/jquery-ui.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/jQuery/Dialog/css/jquery.ui.core.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/jQuery/Dialog/css/jquery.ui.all.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/jQuery/Chosen/chosen.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/Alertify/alertify.core.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/Alertify/alertify.default.css") %>';
        @import '<%= this.ResolveUrl("~/Scripts/jQuery/UploadFile/uploadfile.css") %>';
    </style>

    <script src='<%= this.ResolveUrl("~/Scripts/jQuery/jQuery2/jQuery2.js") %>' type="text/javascript"></script>

